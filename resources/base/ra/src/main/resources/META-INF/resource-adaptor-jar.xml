<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE resource-adaptor-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Resource Adaptor 1.0//EN" "http://java.sun.com/dtd/slee-resource-adaptor-jar_1_0.dtd">

<resource-adaptor-jar>
	<resource-adaptor id="diameter-base_0.8.1_RA">
		<description>Diameter Base Resource Adaptor</description>
		<resource-adaptor-name>DiameterBaseResourceAdaptor</resource-adaptor-name>
		<resource-adaptor-vendor>java.net</resource-adaptor-vendor>
		<resource-adaptor-version>0.8.1</resource-adaptor-version>

		<resource-adaptor-type-ref>
			<resource-adaptor-type-name>Diameter Base Resource Adaptor Type</resource-adaptor-type-name>
			<resource-adaptor-type-vendor>java.net</resource-adaptor-type-vendor>
			<resource-adaptor-type-version>0.8.1</resource-adaptor-type-version>
		</resource-adaptor-type-ref>

		<resource-adaptor-classes>
			<resource-adaptor-class>
				<resource-adaptor-class-name>
          org.mobicents.slee.resource.diameter.base.DiameterBaseResourceAdaptor
        </resource-adaptor-class-name>
			</resource-adaptor-class>
		</resource-adaptor-classes>



		<config-property>
        	<description>List vendor ids, separated by ",", each entry in acc or auth must have entry in this list, matching by index</description>
            <config-property-name>vendorIds</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>193</config-property-value>
        </config-property>
        <config-property>
        	<description>List acctApp ids, separated by ",", each entry in this list must have matching entry in vendor ids, if no venfor, its 0</description>
            <config-property-name>acctAppIds</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>19302</config-property-value>
        </config-property>
        <config-property>
        	<description>List authApp ids, separated by ",", each entry in acc or auth must have entry in this list, matching by index</description>
            <config-property-name>authAppIds</config-property-name>
            <config-property-type>java.lang.String</config-property-type>
            <config-property-value>19302</config-property-value>
        </config-property>
	</resource-adaptor>
</resource-adaptor-jar>

